{
  "name": "emo",
  "lowCaseName": "emo",
  "description": "E-Mail Obfuscation with Javascript",
  "author": "Thomas Jakobi",
  "version": "1.8.5",
  "package": {
    "elements": {
      "plugins": [
        {
          "name": "emo",
          "description": "E-Mail Obfuscation with Javascript",
          "file": "emo.plugin.php",
          "events": [
            "OnLoadWebDocument",
            "OnWebPagePrerender"
          ]
        }
      ]
    },
    "systemSettings": [
      {
        "key": "tpl_only",
        "type": "combo-boolean",
        "area": "system",
        "value": "1"
      },
      {
        "key": "selection_type",
        "area": "system",
        "value": "exclude"
      },
      {
        "key": "selection_range",
        "area": "system",
        "value": ""
      },
      {
        "key": "include_scripts",
        "type": "combo-boolean",
        "area": "system",
        "value": "1"
      },
      {
        "key": "js_path",
        "area": "system",
        "value": ""
      },
      {
        "key": "css_path",
        "area": "system",
        "value": ""
      },
      {
        "key": "no_script_message",
        "area": "system",
        "value": ""
      },
      {
        "key": "debug",
        "type": "combo-boolean",
        "area": "system",
        "value": "0"
      }
    ]
  },
  "dependencies": [
    {
      "name": "php",
      "version": ">=7.2"
    },
    {
      "name": "modx",
      "version": ">=2.6"
    }
  ],
  "build": {
    "readme": "docs/readme.md",
    "license": "docs/license.md",
    "changelog": "docs/changelog.md",
    "setupOptions": {
      "source": "setup.options.php"
    },
    "resolver": {
      "after": [
        "resolve.cleanup.php",
        "resolve.treehillstudio.php"
      ]
    }
  }
}
